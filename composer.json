{
    "name": "lalanikarim/centrifuge-broadcaster",
    "type": "library",
    "description": "Centrifuge broadcaster for laravel >= 8.x",
    "keywords": ["laravel", "centrifuge", "centrifugo", "broadcaster", "websocket"],
    "license": "MIT",
    "authors": [
        {
            "name": "Emprove",
            "email": "skyflap@gmail.com"
        },
        {
            "name": "Nikita Stenin",
            "email": "stenin.nikita@gmail.com"
        },
        {
            "name": "LuisOzuna",
            "email": "lgozuna@gmail.com"
        },
        {
            "name": "Wolfernand",
            "email": "wolfernand@gmail.com"
        },
        {
            "name": "BrianFP",
            "email": "briaan.alexisz@hotmail.com"
        }
    ],
    "homepage": "https://github.com/emprove/centrifuge-broadcaster",
    "support": {
        "issues": "https://github.com/emprove/centrifuge-broadcaster/issues",
        "source": "https://github.com/emprove/centrifuge-broadcaster"
    },
    "require": {
        "php": "7.*|8.*",
        "guzzlehttp/guzzle": "7.*",
        "predis/predis": "1.*",
        "illuminate/broadcasting": "8.*",
        "lcobucci/jwt": "3.3.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.3|^5.0",
        "phpunit/phpunit": "9.*"
    },
    "autoload": {
        "psr-4": {
            "LaraComponents\\Centrifuge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraComponents\\Centrifuge\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraComponents\\Centrifuge\\CentrifugeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
